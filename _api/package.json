{
  "name": "cajuina-cms-auth",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/standalone.js",
    "wrangler:start": "wrangler dev",
    "deploy": "wrangler deploy",
    "check": "pnpm run '/^check:.*/' && pnpm run build",
    "check:env": "pnpm run build && node ./dist/validate-env.js",
    "check:types": "tsc --noEmit",
    "check:audit": "pnpm audit",
    "check:cspell": "cspell --no-progress",
    "check:prettier": "prettier --check .",
    "check:eslint": "eslint .",
    "dev": "wrangler dev",
    "typegen": "wrangler types"
  },
  "dependencies": {
    "cloudflare": "^5.2.0",
    "express": "^4.21.2",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "undici": "^5.29.0",
    "vitest": "^4.0.8",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cajuinasi/prettier-config": "^1.3.1",
    "@cloudflare/vitest-pool-workers": "^0.10.5",
    "@types/express": "^5.0.5",
    "@types/node": "^22.19.0",
    "cspell": "^8.19.4",
    "eslint": "^8.57.1",
    "pino-pretty": "^13.1.2",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "wrangler": "4.46.0"
  },
  "pnpm": {
    "overrides": {
      "cookie@<0.7.0": ">=0.7.0"
    }
  }
}