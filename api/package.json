{
  "name": "sveltia-cms-auth",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/standalone.js",
    "dev": "node --loader ts-node/esm ./standalone.ts",
    "wrangler:start": "wrangler dev",
    "deploy": "wrangler deploy",
    "check": "pnpm run '/^check:.*/' && pnpm run build",
    "check:env": "pnpm run build && node ./dist/validate-env.js",
    "check:types": "tsc --noEmit",
    "check:audit": "pnpm audit",
    "check:cspell": "cspell --no-progress",
    "check:prettier": "prettier --check .",
    "check:eslint": "eslint ."
  },
  "dependencies": {
    "express": "^4.18.2",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "undici": "^5.26.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251014.0",
    "@types/express": "^5.0.3",
    "@types/node": "^22.10.5",
    "cspell": "^8.17.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^50.6.1",
    "pino-pretty": "^13.1.2",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "wrangler": "3.101.0"
  },
  "pnpm": {
    "overrides": {
      "cookie@<0.7.0": ">=0.7.0"
    }
  }
}